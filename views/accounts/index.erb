
  <h2>Your accounts</h2>
  <p><a href="/accounts/new">Add New Account</a></p>
  <% for account in @accounts %>
  <div class="acc-display">
    <a href="/accounts/<%=account.id%>">
      <div class="obj-list">
        <h3><%= account.currency %> </h3>
      </div>
    </a>
    <div class="acc-action">
      <a href="/accounts/<%=account.id%>/edit">Edit Account</a>
      <br>
      <form action="/accounts/<%= account.id %>/delete" method="post">
          <input class="form-bttn" type="submit" value="Delete Account"/>
      </form>
    </div>
  </div>
  <% end %>
