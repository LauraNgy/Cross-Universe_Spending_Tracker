<h2>All tags</h2>

<p><a href="/tags/new">Add New Tag</a></p>

  <%for tag in @tags%>
  <% if tag.name != 'Unknown' %>
  <div class="acc-display">
    <a href="/tags/<%=tag.id%>">
      <div class="obj-list">
        <h3><%= tag.name %> </h3>
      </div>
    </a>
    <div class="acc-action">
      <a href="/tags/<%=tag.id%>/edit">Edit Tag</a>
    </div>
  </div>
  <% end %>
  <% end %>
